{
    "version": "0.2.0",
    "configurations": [


      {
        "type": "node",
        "request": "launch",
        "name": "Launch App.js",
        "sourceMaps": true,
        "program": "${workspaceRoot}/server/app.js",
        "outFiles": [ "${workspaceRoot}/dist/app.js" ]
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Current File",
        "program": "${file}",
        "outFiles": [ "${workspaceRoot}/.compiled/**/*.js" ]
      }
    ]
  }